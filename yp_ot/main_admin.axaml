<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="250"
        x:Class="yp_ot.main_admin"
        Title="Главное меню" Width="200" Height="250">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <TextBlock VerticalAlignment="Center"></TextBlock>
                <Button Background= "Orange" Content="Выйти" Click="Back" Width="60" Height="30" Margin="-30 0 0 10"/>
                <Button Background= "Orange" Content="Услуги" Click="OpenNewWindow" Width="120" Height="30" Margin="-10 40 0 10"/>
                <Button Background= "Orange" Content="Выход" Click="exit" Width="100" Height="30" Margin="0 0 0 10" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
